  "dependencies": {
    "@keyvaluesystems/react-stepper": "^1.0.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.76",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "classnames": "^2.5.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "sass": "^1.70.0",
    "stylelint": "^16.2.0",
    "stylelint-config-clean-order": "^5.4.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.1.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "ct": "npx git-cz",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint \"src/**/*.scss\" --custom-syntax postcss-scss",
    "format": "prettier --write \"**/*.+(ts|tsx|json|yml|yaml|css|md)\"",
    "fix:prettier": "prettier --write --list-different .",
    "fix:eslint": "eslint --ext .tsx,.ts, . --ignore-path .gitignore . --fix",
    "fix:stylelint": "stylelint --fix \"src/**/*.scss\" --custom-syntax postcss-scss",
    "fix": "npm-run-all fix:*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "npm-run-all": "^4.1.5"
  }
